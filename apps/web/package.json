{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm run with-env next build",
    "clean": "rm -rf .next .turbo node_modules",
    "dev": "npm run with-env next dev",
    "lint": "SKIP_ENV_VALIDATION=1 next lint",
    "start": "npm run with-env next start",
    "test:frontend": "npm run with-env vitest run -c ./vitest.config.ts",
    "test:frontend:watch": "npm run with-env vitest -c ./vitest.config.ts",
    "test:server": "npm run with-env -- vitest run -c ./vitest.config.node.ts",
    "test:server:watch": "npm run with-env -- vitest -c ./vitest.config.node.ts",
    "test:unit": "npm run test:frontend && npm run test:server",
    "test:watch": "npm run test:frontend:watch & npm run test:server:watch",
    "test:e2e": "playwright test",
    "test": "npm run test:unit && npm run test:e2e",
    "codegen": "playwright codegen --load-storage playwright/.auth/user.json http://localhost:3000",
    "with-env": "dotenv -e ../../.env --"
  },
